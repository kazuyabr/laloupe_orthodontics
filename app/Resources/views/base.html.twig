<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Néo 3D | Orthodontics{% endblock %}</title>
        {% block stylesheets %}
            <!-- Appel des bibliothèques jQuery -->
            <script type="text/javascript" src="http://code.jquery.com/jquery-latest.min.js"></script>
            <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.14/jquery-ui.min.js"></script>
            <link rel="stylesheet" href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.14/themes/ui-lightness/jquery-ui.css" type="text/css" media="all"/>
            <!-- Appel des bibliothèques Bootstrap -->
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.1/css/font-awesome.min.css">
            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/ortho/css/bootstrap.css') }}" />
            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/ortho/css/bootstrap.min.css') }}" />
            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/ortho/css/bootstrap-theme.css') }}" />
            <!-- Appel de nos feuilles de style CSS -->
            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/ortho/css/stylesheet.css') }}" />
            <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

            <script src="{{ asset('bundles/ortho/js/bootstrap.min.js') }}"></script>

            <script src="https://code.jquery.com/jquery-2.2.3.min.js"
                    integrity="sha256-a23g1Nt4dtEYOj7bR+vTu7+T8VP13humZFBJNIYoEJo=" crossorigin="anonymous"></script>
        {% endblock %}
    </head>
    <body>
        <div class="navbar_homepage">
            <nav class="navbar navbar-inverse">
                <div class="container-fluid">
                    <ul class="nav navbar-nav">

                        <li {% if app.request.get('_route') == 'fos_user_security_logout' %}
                            class="active"{% endif %}>
                            <a href="{{ path('fos_user_security_logout') }}">Déconnexion</a>
                        </li>
                        <li {% if app.request.get('_route') == 'ortho_formulaire' %}
                            class="active"{% endif %}>
                            <a href="{{ path('ortho_formulaire') }}">Formulaire</a>
                        </li>
                        {#<li {% if app.request.get('_route') == 'recap_formulaire' %}
                            class="active"{% endif %}>
                            <a href="{{ path('recap_formulaire') }}">Récap</a>
                        </li>#}
                        <li {% if app.request.get('_route') == 'fos_user_profile_show' %}
                            class="active"{% endif %}>
                            <a href="{{ path('fos_user_profile_show') }}">Profile</a>
                        </li>
                        <li> <a href="http://www.labo-orthodontics.fr" target="_blank">Site du laboratoire</a>
                        </li>
                        <li> <a href="#">Lien 3</a> </li>
                        <li> <a href="#">Lien 4</a> </li>
                        <li> <a href="#">Lien 5</a> </li>
                    </ul>
                </div>
            </nav>
        </div>
    {% block body %}{% endblock %}
    {% block javascript %}
    {% endblock %}
    </body>
</html>